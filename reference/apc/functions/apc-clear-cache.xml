<refentry xml:id="function.apc-clear-cache" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>apc_clear_cache</refname>
  <refpurpose>
   Limpa o cache do APC.
  </refpurpose>
 </refnamediv>
 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>bool</type><methodname>apc_clear_cache</methodname>
   <methodparam choice="opt"><type>string</type><parameter>cache_type</parameter></methodparam>
  </methodsynopsis>
  <para>
   Limpa o cache do usuário/sistema.
  </para>
 </refsect1>
 
 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   &return.success;
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
   <variablelist>
    <varlistentry>
     <term><parameter>cache_type</parameter></term>
     <listitem>
      <para>
       Se <parameter>cache_type</parameter> for "<literal>user</literal>", o
       cache do usuário será limpado; caso contrário, o sistema de cache (arquivos cacheados)
    será limpo.
      </para>
     </listitem>
    </varlistentry>
   </variablelist>
  </para>
 </refsect1>

 <refsect1 role="seealso">
  &reftitle.seealso;
  <para>
   <simplelist>
    <member><function>apc_cache_info</function></member>
   </simplelist>
  </para>
 </refsect1>
 
</refentry>

