 <refentry xml:id="function.imagecolorstotal" xmlns="http://docbook.org/ns/docbook">
  <refnamediv>
   <refname>imagecolorstotal</refname>
   <refpurpose>Retorna o número de cores na paleta de uma imagem</refpurpose>
  </refnamediv>
 <refsect1 role="description">
  &reftitle.description;
    <methodsynopsis>
     <type>int</type><methodname>imagecolorstotal</methodname>
     <methodparam><type>resource</type><parameter>image</parameter></methodparam>
    </methodsynopsis>
   <para>
    Retorna o número de cores na paleta da imagem.
   </para>
 </refsect1>
 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
   <variablelist>
    &gd.image.description;
   </variablelist>
  </para>
 </refsect1>
 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Retorna o número de cores na especificada paleta da imagem ou 0 para
   imagens truecolor.
  </para>
 </refsect1>
 <refsect1 role="seealso">
  &reftitle.seealso;
   <para>
     <simplelist>
      <member><function>imagecolorat</function></member>
      <member><function>imagecolorsforindex</function></member>
      <member><function>imageistruecolor</function></member>
     </simplelist>
   </para>
  </refsect1>
 </refentry>

