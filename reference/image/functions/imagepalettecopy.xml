<refentry xml:id="function.imagepalettecopy" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>imagepalettecopy</refname>
  <refpurpose>Copia a paleta de uma imagem para a outra</refpurpose>
 </refnamediv>
 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>void</type><methodname>imagepalettecopy</methodname>
   <methodparam><type>resource</type><parameter>destination</parameter></methodparam>
   <methodparam><type>resource</type><parameter>source</parameter></methodparam>
  </methodsynopsis>
  <para>
   <function>imagepalettecopy</function> copia a paleta da imagem 
   de <parameter>source</parameter> para a imagem <parameter>destination</parameter>.
  </para>
 </refsect1>
 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
   <variablelist>
    <varlistentry>
     <term><parameter>destination</parameter></term>
     <listitem>
      <para>
       O resource da imagem de destino
      </para>
     </listitem>
    </varlistentry>
    <varlistentry>
     <term><parameter>source</parameter></term>
     <listitem>
      <para>
       O resource do fonte da imagem
      </para>
     </listitem>
    </varlistentry>
   </variablelist>
  </para>
 </refsect1>
 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   &return.void;
  </para>
 </refsect1>
</refentry>

