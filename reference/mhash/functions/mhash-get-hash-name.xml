<refentry xml:id="function.mhash-get-hash-name" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>mhash_get_hash_name</refname>
  <refpurpose>Ler o nome da hash especificada</refpurpose>
 </refnamediv>
 
 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>string</type><methodname>mhash_get_hash_name</methodname>
   <methodparam><type>int</type><parameter>hash</parameter></methodparam>
  </methodsynopsis>
  <para>
   Obtém o nome da especificada <parameter>hash</parameter>.
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
   <variablelist>
    <varlistentry>
     <term><parameter>hash</parameter></term>
     <listitem>
      <para>
       O hash id. Uma das constantes <literal>MHASH_XXX</literal>.
      </para>
     </listitem>
    </varlistentry>
   </variablelist>
  </para>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Retorna o nome da hash ou &false;, se a hash não existe.
  </para>
 </refsect1>

 <refsect1 role="examples">
  &reftitle.examples;
  <para>
   <example>
    <title>Exemplo do <function>mhash_get_hash_name</function></title>
    <programlisting role="php">
<![CDATA[
<?php

echo mhash_get_hash_name(MHASH_MD5);

?>
]]>
    </programlisting>
   </example>
  </para>
 </refsect1>
</refentry>

