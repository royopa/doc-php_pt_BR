<refentry xml:id="function.getmyinode" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>getmyinode</refname>
  <refpurpose>Obtém o inode do script atual</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
   <methodsynopsis>
    <type>int</type><methodname>getmyinode</methodname>
    <void/>
   </methodsynopsis>
  <para>
   Obtém o inode do script atual.
  </para>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Retorna o inode do atual script como um inteiro, ou &false; em erro.
  </para>
 </refsect1>

 <refsect1 role="notes">
  &reftitle.notes;
  &note.no-windows;
 </refsect1>
</refentry>

