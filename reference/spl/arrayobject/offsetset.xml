<refentry xml:id="arrayobject.offsetset" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>ArrayObject::offsetSet</refname>
  <refpurpose>Define o valor do índice especificado por $index para $newval</refpurpose>
 </refnamediv>
 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>void</type><methodname>ArrayObject::offsetSet</methodname>
   <methodparam><type>mixed</type><parameter>index</parameter></methodparam>
   <methodparam><type>mixed</type><parameter>newval</parameter></methodparam>
  </methodsynopsis>

   &warn.undocumented.func;

  <para>
   Sets the value at the specified index to newval.
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
   <variablelist>
    <varlistentry>
     <term><parameter>index</parameter></term>
     <listitem>
      <para>
       O índice a ser definido.
      </para>
     </listitem>
    </varlistentry>
    <varlistentry>
     <term><parameter>newval</parameter></term>
     <listitem>
      <para>
       O novo valor para o <parameter>index</parameter>.
      </para>
     </listitem>
    </varlistentry>
   </variablelist>
  </para>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   &return.void;
  </para>
 </refsect1>

</refentry>

